{
  "name": "word-jumble",
  "version": "1.0.0",
  "description": "Try and make more words than your opponent within the time limit!",
  "scripts": {
    "build": "webpack --config ./config/webpack/config.js",
    "start": "node server",
    "watch": "concurrently --prefix \"[{name}]\" --names \"webpack,express\" \"webpack --config ./config/webpack/config.js --watch\" \"node server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicgordon/word-jumble.git"
  },
  "author": "Nic Gordon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicgordon/word-jumble/issues"
  },
  "homepage": "https://github.com/nicgordon/word-jumble#readme",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "express": "^4.14.0",
    "lodash": "^4.16.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.5.1",
    "socket.io-client": "^1.5.1",
    "tocktimer": "^1.0.7"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.3",
    "webpack-md5-hash": "0.0.5"
  },
  "engines": {
    "node": ">=6"
  }
}
